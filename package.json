{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "description": "Functional components built with Vue 3",
    "version": "0.1.0-rc.3",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "main": "./dist/out-of-the-vue.umd.js",
    "module": "./dist/out-of-the-vue.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.es.js",
            "require": "./dist/out-of-the-vue.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test-coverage": "vitest run --coverage",
        "release": "npm install && npm run build && npm publish"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/michaelverschoof/out-of-the-vue.git"
    },
    "bugs": "https://github.com/michaelverschoof/out-of-the-vue/issues",
    "keywords": [
        "functional",
        "vue",
        "vue3",
        "mobile",
        "frontend",
        "component",
        "components"
    ],
    "dependencies": {
        "vue": "^3"
    },
    "devDependencies": {
        "@types/node": "^17.0.43",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/test-utils": "^2.0.1",
        "c8": "^7.11.3",
        "happy-dom": "^5.3.1",
        "jsdom": "^19.0.0",
        "sass": "^1.52.3",
        "typescript": "^4.7.3",
        "vite": "^2.9.12",
        "vite-plugin-dts": "^1.2.0",
        "vitest": "^0.15.1"
    },
    "peerDependencies": {
        "vue": "^3"
    }
}
