{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "version": "0.1.0-beta.24",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "main": "./dist/out-of-the-vue.umd.js",
    "module": "./dist/out-of-the-vue.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.es.js",
            "require": "./dist/out-of-the-vue.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build && vue-tsc --emitDeclarationOnly --project tsconfig.build.json",
        "preview": "vite preview",
        "test": "vitest",
        "test-coverage": "vitest run --coverage"
    },
    "dependencies": {
        "vue": "^3.2.36"
    },
    "devDependencies": {
        "@types/node": "^17.0.38",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/test-utils": "^2.0.0",
        "c8": "^7.11.3",
        "happy-dom": "^5.0.0",
        "jsdom": "^19.0.0",
        "sass": "^1.52.1",
        "typescript": "^4.7.2",
        "vite": "^2.9.9",
        "vitest": "^0.13.1",
        "vue-tsc": "^0.35.2"
    },
    "peerDependencies": {
        "vue": "^3"
    }
}
