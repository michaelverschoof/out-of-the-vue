{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "description": "Functional UX components built with Vue 3",
    "version": "0.1.0-rc.36",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/michaelverschoof/out-of-the-vue.git"
    },
    "main": "./dist/out-of-the-vue.umd.js",
    "module": "./dist/out-of-the-vue.es.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.es.js",
            "require": "./dist/out-of-the-vue.umd.js"
        },
        "./dist/out-of-the-vue.css": "./dist/out-of-the-vue.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "release": "npm install && npm run build && npm publish",
        "story": "histoire build",
        "story:watch": "histoire dev",
        "story:preview": "histoire preview"
    },
    "bugs": "https://github.com/michaelverschoof/out-of-the-vue/issues",
    "keywords": [
        "functional",
        "UX",
        "vue",
        "vue3",
        "mobile",
        "frontend",
        "component",
        "components"
    ],
    "devDependencies": {
        "@histoire/plugin-vue": "^0.11.6",
        "@iconify/vue": "^4.0.0",
        "@types/node": "^18.11.9",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vue/test-utils": "^2.2.1",
        "c8": "^7.12.0",
        "happy-dom": "^7.6.7",
        "histoire": "^0.11.6",
        "jsdom": "^20.0.2",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "sass": "^1.56.1",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.8.4",
        "vite": "^3.2.3",
        "vite-plugin-dts": "^1.7.0",
        "vitest": "^0.25.1",
        "vue": "^3.2.44"
    },
    "peerDependencies": {
        "vue": "^3"
    },
    "dependencies": {
        "autoprefixer": "^10.4.13"
    }
}
