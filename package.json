{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "description": "Functional UX components built with Vue 3",
    "version": "0.1.0-rc.28",
    "license": "MIT",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/michaelverschoof/out-of-the-vue.git"
    },
    "main": "./dist/out-of-the-vue.umd.js",
    "module": "./dist/out-of-the-vue.es.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.es.js",
            "require": "./dist/out-of-the-vue.umd.js"
        },
        "./dist/out-of-the-vue.css": "./dist/out-of-the-vue.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "release": "npm install && npm run build && npm publish"
    },
    "bugs": "https://github.com/michaelverschoof/out-of-the-vue/issues",
    "keywords": [
        "functional",
        "UX",
        "vue",
        "vue3",
        "mobile",
        "frontend",
        "component",
        "components"
    ],
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/test-utils": "^2.0.0",
        "c8": "^7.11.3",
        "happy-dom": "^5.3.2",
        "jsdom": "^20.0.0",
        "sass": "^1.53.0",
        "typescript": "^4.7.4",
        "vite": "^2.9.13",
        "vite-plugin-dts": "^1.2.0",
        "vitest": "^0.16.0",
        "vue": "^3.2.37"
    },
    "peerDependencies": {
        "vue": "^3"
    }
}
