{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "description": "Functional UX components built with Vue 3",
    "version": "0.1.0-rc.52",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/michaelverschoof/out-of-the-vue.git"
    },
    "main": "./dist/out-of-the-vue.umd.cjs",
    "module": "./dist/out-of-the-vue.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "src",
        "!**/*.story.vue",
        "!**/*.test.ts",
        "!src/main.ts",
        "!src/out-of-the-vue.vue"
    ],
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.js",
            "require": "./dist/out-of-the-vue.umd.cjs"
        },
        "./dist/out-of-the-vue.css": "./dist/out-of-the-vue.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:once": "vitest run",
        "test:coverage": "vitest run --coverage",
        "release": "npm install && npm run build && npm publish",
        "story": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview"
    },
    "bugs": "https://github.com/michaelverschoof/out-of-the-vue/issues",
    "keywords": [
        "functional",
        "UX",
        "vue",
        "vue3",
        "mobile",
        "frontend",
        "component",
        "components"
    ],
    "devDependencies": {
        "@histoire/plugin-vue": "0.17.1",
        "@iconify/vue": "4.1.1",
        "@types/node": "20.6.0",
        "@vitejs/plugin-vue": "4.3.4",
        "@vitest/coverage-c8": "0.33.0",
        "@vue/test-utils": "2.4.1",
        "autoprefixer": "10.4.15",
        "c8": "8.0.1",
        "happy-dom": "11.0.2",
        "histoire": "0.17.0",
        "jsdom": "22.1.0",
        "prettier": "3.0.3",
        "prettier-plugin-tailwindcss": "0.5.4",
        "tailwindcss": "3.3.3",
        "typescript": "5.2.2",
        "vite": "4.4.9",
        "vite-plugin-dts": "3.5.3",
        "vitest": "^0.34.4",
        "vue": "3.3.4"
    },
    "peerDependencies": {
        "vue": "^3"
    }
}
