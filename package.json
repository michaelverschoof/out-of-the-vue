{
    "name": "out-of-the-vue",
    "author": "Michael Verschoof",
    "description": "Functional UX components built with Vue 3",
    "version": "0.1.0-rc.46",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/michaelverschoof/out-of-the-vue.git"
    },
    "main": "./dist/out-of-the-vue.umd.cjs",
    "module": "./dist/out-of-the-vue.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "src",
        "!**/*.story.vue",
        "!**/*.test.ts",
        "!src/main.ts",
        "!src/out-of-the-vue.vue"
    ],
    "exports": {
        ".": {
            "import": "./dist/out-of-the-vue.js",
            "require": "./dist/out-of-the-vue.umd.cjs"
        },
        "./dist/out-of-the-vue.css": "./dist/out-of-the-vue.css"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:once": "vitest run",
        "test:coverage": "vitest run --coverage",
        "release": "npm install && npm run build && npm publish",
        "story": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview"
    },
    "bugs": "https://github.com/michaelverschoof/out-of-the-vue/issues",
    "keywords": [
        "functional",
        "UX",
        "vue",
        "vue3",
        "mobile",
        "frontend",
        "component",
        "components"
    ],
    "devDependencies": {
        "@histoire/plugin-vue": "^0.15.8",
        "@iconify/vue": "^4.0.2",
        "@types/node": "^18.11.14",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vitest/coverage-c8": "^0.29.2",
        "@vue/test-utils": "^2.3.1",
        "autoprefixer": "^10.4.13",
        "c8": "^7.12.0",
        "happy-dom": "^8.9.0",
        "histoire": "^0.15.9",
        "jsdom": "^20.0.3",
        "prettier": "^2.8.1",
        "prettier-plugin-tailwindcss": "^0.2.1",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.4",
        "vite": "^3.2.3",
        "vite-plugin-dts": "1.7.1",
        "vitest": "^0.29.2",
        "vue": "^3.2.47"
    },
    "peerDependencies": {
        "vue": "^3"
    }
}
